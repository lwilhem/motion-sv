{
  "name": "motion-sv",
  "type": "module",
  "version": "0.0.1",
  "keywords": [
    "svelte"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "!dist/**/*.spec.*",
    "!dist/**/*.test.*",
    "dist"
  ],
  "scripts": {
    "prepare": "svelte-kit sync || echo ''",
    "dev": "vite dev --port 3000",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "build": "vite build && npm run prepack",
    "preview": "vite preview"
  },
  "svelte": "./dist/index.js",
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "motion-dom": "catalog:frontend",
    "motion-utils": "catalog:frontend"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@sveltejs/adapter-auto": "catalog:build",
    "@sveltejs/kit": "catalog:build",
    "@sveltejs/package": "catalog:build",
    "@sveltejs/vite-plugin-svelte": "catalog:build",
    "eslint": "catalog:dev",
    "eslint-plugin-format": "catalog:dev",
    "eslint-plugin-svelte": "catalog:dev",
    "publint": "catalog:dev",
    "svelte": "catalog:frontend",
    "svelte-check": "catalog:dev",
    "typescript": "catalog:dev",
    "vite": "catalog:build"
  }
}
